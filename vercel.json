{
  "builds": [
    {
      "src": "chat_backend/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "build/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "build/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "chat_backend/wsgi.py"
    }
  ]
}
